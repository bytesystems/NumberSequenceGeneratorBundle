{
    "name": "bytesystems/number-sequence-generator",
    "description": "A generator for human readable number sequences",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Heim",
            "email": "heim@bytesystems.de"
        }
    ],
    "require": {
        "php": "^7.3|^8.0|^8.1",
        "symfony/config": "^4.4|^5.0|^6.0",
        "symfony/dependency-injection": "^4.4|^5.0|^6.0",
        "doctrine/orm": "^2.6",
        "symfony/property-access": "^5.2|^6.0",
        "doctrine/migrations": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Bytesystems\\NumberGeneratorBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bytesystems\\NumberGeneratorBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.2",
        "symfony/http-kernel": "^4.4|^5.0",
        "doctrine/doctrine-bundle": "^2.3",
        "symfony/framework-bundle": "^4.4|^5.0",
        "symfony/yaml": "^5.2",
        "liip/test-fixtures-bundle": "^2",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "phpstan/phpstan": "^0.12.83"
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan "
    }
}
